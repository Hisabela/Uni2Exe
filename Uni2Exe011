import java.io.IOException;
import java.util.Scanner;

public class Main {
 
    public static void main(String[] args) throws IOException {
 Scanner teclado = new Scanner (System.in);

 double A = teclado.nextDouble();

 double B = teclado.nextDouble();

 double C = teclado.nextDouble();

double resultadoA = (A*C)/2;
double resultadoB = 3.14159 * Math.pow(C, 2);
double resultadoC = ((A+B)*C)/2;
double resultadoD = Math.pow(B,2);
double resultadoE = A*B;

System.out.println("\nTRIANGULO: "+String.format("%.3f", resultadoA) + "\nCIRCULO: "+String.format("%.3f", resultadoB) +"\nTRAPEZIO: "+String.format("%.3f", resultadoC) +"\nQUADRADO: "+String.format("%.3f", resultadoD) +"\nRETANGULO: " +String.format("%.3f", resultadoE));

teclado.close();

 
    }
 
}
