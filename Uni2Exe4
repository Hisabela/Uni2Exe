import java.util.Scanner;
public class Uni2Exe04 {

    //Leia dois valores de ponto flutuante e calcule a média ponderada sabendo
    //que a nota A tem peso 3.5 e nota B 7.5 (a soma dos pesos é 11), sendo que a nota vai de 0.0 a 10.0.

    public static void main(String[] args) {

        Scanner teclado = new Scanner(System.in);
       
    System.out.println("Digite sua primeira nota: ");
    double nota1 = teclado.nextDouble();

    System.out.println("Digite sua primeira nota: ");
    double nota2 = teclado.nextDouble();

    double notaA = nota1*3.5;

    double notaB = nota2*7.5;

    double notaFinal = ((notaA + notaB)/11);

    System.out.println("A soma da nota A " +notaA+ " mais a soma da nota B " +notaB+ " fica " + notaFinal);
    teclado.close();

    }

}

